package ejercicioC3;

public class Rectangulo {
	
	private int x1;
	private int y1;
	private int x2;
	private int y2;
	
	public Rectangulo(int x1, int y1, int x2, int y2) {
		
		if((x1 < x2) && (y1 < y2)) {
		this.x1 = x1;
		this.x2 = x2;
		this.y1 = y1;
		this.y2 = y2;
		}else {
			System.err.println("ERROR al instanciar Rectangulo...");
		}
	}
	
	public int getX1 () {
		return x1;
	}
	
	public int getY1 () {
		return x1;
	}
	
	public void setX2(int X2) {
		this.x2 = X2;
	}
	
	public void setY2(int Y2) {
		this.x2 = Y2;
	}
}
